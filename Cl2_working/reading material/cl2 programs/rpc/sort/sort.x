/*  RPC Register file for sorting the number
   Roll No:4169 (BE-I)
*/

/*
 * The Sort procedure receives an array of real
 * numbers and returns the Sort of their
 * values. This program handles a maximum of
 * 200 numbers.
 */
const MAXAVGSIZE  = 200;

struct input_data {
  double input_data <200>;
};

typedef struct input_data input_data;

program SORTPROG {
    version SORTVERS {
        input_data SORT(input_data)= 1;
    } = 1;
} = 22855;
